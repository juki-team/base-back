{
  "name": "@juki-team/base-back",
  "version": "0.0.189",
  "license": "MIT",
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./dist/main.js",
      "types": "./dist/index.d.ts"
    },
    "./services/aws/apigatewaymanagementapi": {
      "require": "./dist/services/aws/apigatewaymanagementapi.js",
      "import": "./dist/services/aws/apigatewaymanagementapi.js",
      "types": "./dist/services/aws/apigatewaymanagementapi.d.ts"
    },
    "./services/aws/ec2": {
      "require": "./dist/services/aws/ec2.js",
      "import": "./dist/services/aws/ec2.js",
      "types": "./dist/services/aws/ec2.d.ts"
    },
    "./services/aws/ecs": {
      "require": "./dist/services/aws/ecs.js",
      "import": "./dist/services/aws/ecs.js",
      "types": "./dist/services/aws/ecs.d.ts"
    },
    "./services/aws/s3": {
      "require": "./dist/services/aws/s3.js",
      "import": "./dist/services/aws/s3.js",
      "types": "./dist/services/aws/s3.d.ts"
    },
    "./services/aws/sqs": {
      "require": "./dist/services/aws/sqs.js",
      "import": "./dist/services/aws/sqs.js",
      "types": "./dist/services/aws/sqs.d.ts"
    }
  },
  "scripts": {
    "start": "export NODE_ENV=production && ts-node ./src/app.ts",
    "dev": "export VERSION=$(node -p \"require('./package.json').version\") && ts-node-dev ./src/app.ts",
    "pull": "yarn add @juki-team/commons --peer",
    "rollup:build": "rollup -c",
    "build": "rm -rf dist && yarn rollup:build",
    "git-push": "f() { param=${1}; message=${param:-chore}; echo ${message}; git add .; echo $message | git commit -F -; yarn publish --patch --no-git-tag-version; version=\"v$(node -p \"require('./package.json').version\")\"; git add .; echo $version | git commit -F -; git push origin main; echo $version | git tag -a $(echo $version) -F -; git push origin --tags; }; f",
    "push": "f() { yarn build && yarn git-push \"${1}\"; }; f"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.709.0",
    "@aws-sdk/client-ec2": "^3.569.0",
    "@aws-sdk/client-ecs": "^3.569.0",
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/client-sqs": "^3.569.0",
    "@juki-team/commons": "^0.0.690",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^18.14.6",
    "@types/uuid": "^9.0.8",
    "express": "^4.21.2",
    "rollup": "^2.70.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.709.0",
    "@aws-sdk/client-ec2": "^3.569.0",
    "@aws-sdk/client-ecs": "^3.569.0",
    "@aws-sdk/client-s3": "^3.569.0",
    "@aws-sdk/client-sqs": "^3.569.0",
    "@juki-team/commons": "^0.0.686",
    "express": "^4.21.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "mime-types": "^2.1.35",
    "uuid": "^11.0.3"
  }
}
